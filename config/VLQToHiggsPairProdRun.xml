<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE JobConfiguration PUBLIC "" "JobConfig.dtd"
[
<!ENTITY WJets SYSTEM "/nfs/dust/cms/user/nowatsd/sFrameNew/SFRAME/uhh2/common/datasets/MC_WJets_LNu_20x25.xml">
<!ENTITY ZJets SYSTEM "/nfs/dust/cms/user/nowatsd/sFrameNew/SFRAME/uhh2/common/datasets/MC_ZJets_LNu_20x25.xml">
<!ENTITY TTbar SYSTEM "/nfs/dust/cms/user/nowatsd/sFrameNew/SFRAME/uhh2/common/datasets/MC_TTJets_20x25.xml">
<!ENTITY TpTp_M1000 SYSTEM "/nfs/dust/cms/user/nowatsd/sFrameNew/SFRAME/uhh2/common/datasets/MC_TpTp_M1000_20x25.xml">
]
>



<JobConfiguration JobName="VLQToHPairProdCycleJob" OutputLevel="INFO">
    <Library Name="libSUHH2vlqToHPairProd"/>
    <Package Name="SUHH2vlqToHPairProd.par" />

   <Cycle Name="uhh2::AnalysisModuleRunner" OutputDirectory="./output/" PostFix="" TargetLumi="1." >
   <!-- <Cycle Name="uhh2::AnalysisModuleRunner"  RunMode="PROOF" ProofServer="lite://"  ProofWorkDir="/nfs/dust/cms/user/ottjoc/proof-wd/" ProofNodes="2" OutputDirectory="./" PostFix="" TargetLumi="1" > -->
        <InputData Lumi="1." NEventsMax="1000" Type="MC" Version="TpTp_M1000" Cacheable="False">
            &TpTp_M1000;
            <!-- Lumi="4796772" NEventsMax="-1" -->
            <InputTree Name="AnalysisTree" />
            <!-- <OutputTree Name="AnalysisTree" /> -->

        </InputData>
        <InputData Lumi="1." NEventsMax="1000" Type="MC" Version="TTJets" Cacheable="False">
            &TTbar;
            <InputTree Name="AnalysisTree" />
            <!-- <OutputTree Name="AnalysisTree" /> -->

        </InputData>
        <!-- Lumi="3156.803" NEventsMax="2546000" -->
        <!-- <InputData Lumi="1" NEventsMax="1000" Type="MC" Version="WJets" Cacheable="False">
            &WJets;
            <InputTree Name="AnalysisTree" />
            <OutputTree Name="AnalysisTree" />      

        </InputData> -->
       <!--  <InputData Lumi="1" NEventsMax="1000" Type="MC" Version="ZJets" Cacheable="False">
            &ZJets;
            <InputTree Name="AnalysisTree" />
            <OutputTree Name="AnalysisTree" />      

        </InputData>  -->

        <!-- <OutputTree Name="AnalysisTree" /> -->      
            
        <UserConfig>
            <!-- define which collections to read from the input. Only specify what you need to save I/O time -->
            <Item Name="PrimaryVertexCollection" Value="offlineSlimmedPrimaryVertices" /> 
            <Item Name="ElectronCollection" Value="slimmedElectrons" />
            <Item Name="MuonCollection" Value="slimmedMuons" /> 
            <Item Name="TauCollection" Value="slimmedTaus" />
            <Item Name="JetCollection" Value="slimmedJets" />
            <Item Name="METName" Value="slimmedMETs" />
            <Item Name="GenParticleCollection" Value="GenParticles" />
            <Item Name="GenJetCollection" Value="slimmedGenJets" />
            <!-- <Item Name="TopJetCollection" Value="patJetsCMSTopTagCHSPacked" /> -->
            <Item Name="TopJetCollection" Value="patJetsCmsTopTagCHSPacked" />
            <Item Name="GenTopJetCollection" Value="cmsTopTagGen" />


            <!-- The list of available collections to read in corresponds to the pointer members in event. The 'Name's to use here in the configuration are:
            
                 PrimaryVertexCollection
                 ElectronCollection
                 MuonCollection
                 TauCollection
                 PhotonCollection
                 JetCollection
                 TopJetCollection
                 METName
                 
                 GenInfoName
                 GenJetCollection
                 GenTopJetCollection
                 GenParticleCollection
                 
                 Note that the last four are only read in for MC (InputData Type="MC"), so it is save to specify
                 them for real data, although those branches do not exist.
                 
                 As 'Value', always use the branch name (if in doubt, open the input file in ROOT).
                 
                 By default, triggers are read. To explicitly disable reading triggers, use:
                 <Item Name="readTrigger" Value="false" />
            -->
            
            <!-- the class name of the AnalysisModule subclasses to run: -->
            <Item Name="AnalysisModule" Value="VLQToHiggsPairProdAnalysis" />

            <!-- tell AnalysisModuleRunner NOT to use the MC event weight from SFrame; rather let
                 MCLumiWeight (called via CommonModules) calculate the MC event weight. The MC
                 event weight assigned by MCLumiWeight is InputData.Lumi / Cycle.TargetLumi. -->
            <Item Name="use_sframe_weight" Value="false" />
            
            <!-- configuration for MCPileupReweight (run as part of CommonModules): -->
            <Item Name="pileup_directory" Value="" />
            
            <!-- set this to the name of an AndSelection to control what is written to the output. If not given, *all* events are written to the output tree. -->
            <Item Name="TestKey" Value="TestKeyValue" />
        </UserConfig>
    </Cycle>
</JobConfiguration>
